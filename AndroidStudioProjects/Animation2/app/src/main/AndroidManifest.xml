<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ashwini.animation2" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.example.ashwini.animation2.FadeInActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.FadeOutActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.CrossfadeActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.BlinkActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.ZoomInActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.ZoomOutActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.RotateActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.MoveActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.SlideUpActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.SlideDownActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.BounceActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.SequentialActivity"
            android:label="@string/app_name" />
        <activity
            android:name="com.example.ashwini.animation2.TogetherActivity"
            android:label="@string/app_name" />

    </application>

</manifest>
